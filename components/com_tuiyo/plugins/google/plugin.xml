	<authorEmail>admin@tuiyo.co.uk</authorEmail>
	<authorUrl>www.tuiyo.co.uk</authorUrl>
	<version>1.0.0</version>
	<description>Google service and  Jommla</description>
	<params group="plugin">
		<param name="username" type="text" default="" label="Username"  />
		<param name="password" type="password" default="" label="Password"  />
	</params>
	<params group="privacy"></params>
	<params group="profilepicture"></params>
	<params group="administrator">
			<param type="text" class="TuiyoFormText" name="gMapAPIKey" label="Google Maps API Key" />
			<param type="text" class="TuiyoFormText" name="gAnalyticsAccountName" label="Google Analytics Account Name"  />
			<param type="text" class="TuiyoFormText" name="gAnalyticsAccountID" label="Google Analytics Account ID" />
			<param type="text" class="TuiyoFormText" name="gAnalyticsWebPropertyID" label="Google Analytics WebPropertyID"  />
			<param type="text" class="TuiyoFormText" name="gBuzzAPI" label="Google Buzz API" />
			<param type="text" class="TuiyoFormText" name="gTranslateAPI" label="Google Translate API" />
	</params>
</install>